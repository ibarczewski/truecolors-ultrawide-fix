{
  "name": "webex-bot-github-enterprise",
  "version": "0.0.1",
  "description": "Github Enterprise Webex bot",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start-qa": "concurrently \"npm:start\" \"npx localtunnel --port 7001 --subdomain wwt-githubqa\"",
    "build": "rm -rf dist && tsc",
    "test": "jest",
    "dev": "nodemon -e ts --exec \"npm run build && npm run start\"",
    "lint": "eslint --fix src",
    "deploy:ec2:dev": "sh ./deploy/ec2/deploy_ec2.sh dev",
    "deploy:ec2:qa": "sh ./deploy/ec2/deploy_ec2.sh qa",
    "deploy:ec2:demo": "sh ./deploy/ec2/deploy_ec2.sh demo",
    "bundle": "cp package.json dist/package.json && cp package-lock.json dist/package-lock.json && cd dist && npm ci --production",
    "preinstall": "source $HOME/.nvm/nvm.sh; nvm use"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sqbu-github.cisco.com/ibarczew/github-bot.git"
  },
  "keywords": [
    "Cisco",
    "Webex",
    "webex-bot-framework"
  ],
  "author": "WWT Integrations team",
  "license": "ISC",
  "bugs": {
    "url": "https://sqbu-github.cisco.com/ibarczew/github-bot/issues"
  },
  "homepage": "https://sqbu-github.cisco.com/ibarczew/github-bot#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.14.0",
    "node-fetch": "^2.6.6",
    "webex-node-bot-framework": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@octokit/webhooks-types": "^4.17.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}